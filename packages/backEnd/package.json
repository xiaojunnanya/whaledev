  "dependencies": {
    "@nestjs/axios": "^4.0.0",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^4.0.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/mapped-types": "*",
    "@nestjs/platform-express": "^10.0.0",
    "@prisma/client": "^5.21.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "ejs": "^3.1.10",
    "ioredis": "^5.4.1",
    "nodemailer": "^6.9.15",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.8.1",
    "svg-captcha": "^1.4.0",
    "uuid": "^11.0.2"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
